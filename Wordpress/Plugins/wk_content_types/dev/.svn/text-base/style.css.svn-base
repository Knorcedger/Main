/* @override http://techzoo.o-some.com/wp-content/plugins/wk_content_types/style.css */

#misc-publishing-actions div.featured.misc-pub-section {
	position: relative;
	}

#misc-publishing-actions div.featured.misc-pub-section ul {
	position: absolute; top: 3px; left: 5.1em;
	}

div#photo_box div.new_photo,
div#photo_box div.new_featured_photo {
	margin: 14px 0;
	}


div#photo_box .button {
	margin-left: 10px;
	}
div#photo_box .howto {
	margin-top: 10px;
	}
div#photo_box .howto.separator {
	margin-top: 10px;
	padding-bottom: 10px;
	margin-left: -6px; margin-right: -6px;
	padding-left: 6px; padding-right: 6px;
	border-bottom: 1px solid #eee;
	}
div#photo_box div.new_featured_photo .howto.separator {
	border: none;
	padding-bottom: 0px;
	}

div#photo_box a.add.button {
	display: block;
	width: 3em;
	text-align: center;
	margin: 5px 0;
	}

div.ac_results li.ac_over {
	color: #ffffff;
	background: #257ea8;
	}